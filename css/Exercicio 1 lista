1.c
#include <stdio.h>
int main ()
{
    printf ( "%d",4);
    printf ( "%d",2);
    return 0;
}

1.d
# include <stdio.h>
int main () 
{
    int a ;
    scanf ( " %i" ,&a );
    printf ( " %i",a );
    return 0;
}
